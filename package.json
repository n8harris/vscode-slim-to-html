{
  "name": "slim-to-html",
  "displayName": "Slim To Html",
  "description": "",
  "version": "0.0.2",
  "publisher": "n8harris",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.slim-to-html"
  ],
  "main": "./out/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.slim-to-html",
        "key": "ctrl+shift+/",
        "when": "editorTextFocus && editorLangId == 'slim'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "requestify": "^0.1.16"
  }
}
